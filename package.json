{
  "name": "sba-gov",
  "version": "0.0.1",
  "description": "The application repository for sba.gov",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node src/app.js --presets env || true",
    "nodemonstart": "nodemon src/app.js --exec babel-node  --presets env",
    "prestart": "npm run clean && npm run lint",
    "build": "scripts/build.sh all",
    "clean": "rm -rf dist && rm -rf public/build",
    "format": "prettier --write '{src,test}/**/*.{js,json,jsx,scss}'",
    "lint": "eslint src",
    "test": "NODE_ENV=test npm run test-jest && NODE_ENV=test npm run test-mocha",
    "test-jest": "NODE_ENV=test jest -w 1 test/jest/",
    "test-mocha":
      "NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill --require ignore-styles ./test/mocha/helpers/globals.js \"test/mocha/**/*.test.js\"",
    "test-int-mocha":
      "NODE_ENV=test mocha --compilers js:babel-core/register  --require babel-polyfill --require  ignore-styles ./test/mocha/helpers/globals.js \"test/mocha/**/*.integration-test.js\"",
    "test-format": "prettier --write '{src,test}/**/*.{js,json,jsx,scss}'",
    "precommit": "npm run lint && npm run test && lint-staged",
    "clear-cache": "curl localhost:3000/actions/clearcache"
  },
  "repository": {
    "url": "git+https://github.com/sba-ds/sba-gov.git",
    "type": "git"
  },
  "keywords": ["sba", "ds", "node", "react"],
  "author": "SBA Digital Services",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sba-ds/sba-gov/issues"
  },
  "homepage": "https://github.com/sba-ds/sba-gov#readme",
  "dependencies": {
    "accept-language-parser": "^1.4.1",
    "aws-sdk": "^2.177.0",
    "aws-serverless-express": "^3.2.0",
    "axios": "0.16.1",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "cheerio": "0.22.0",
    "compression": "^1.7.3",
    "config": "1.24.0",
    "cookie-parser": "1.4.3",
    "cron": "1.2.1",
    "css-loader": "0.28.2",
    "dotenv": "^4.0.0",
    "express": "4.14.0",
    "html-to-text": "3.0.0",
    "http-status-codes": "1.0.6",
    "json2csv": "^3.7.3",
    "lodash": "4.17.2",
    "moment": "2.17.1",
    "moment-timezone": "0.5.13",
    "mysql": "2.12.0",
    "mysql2": "^1.5.1",
    "node-sass": "4.5.3",
    "nodemailer": "4.0.1",
    "pug": "2.0.0-beta11",
    "querystring": "0.2.0",
    "sequelize": "3.30.2",
    "url-loader": "0.5.7",
    "uuid": "3.0.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.8.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "4.1.8",
    "babel-jest": "16.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^1.0.0",
    "dynamic-public-path-webpack-plugin": "^1.0.3",
    "element-overlap": "1.0.1",
    "enzyme": "2.7.1",
    "esformatter-ignore": "0.1.3",
    "esformatter-jsx": "8.0.0",
    "eslint": "3.10.1",
    "eslint-plugin-react": "7.0.1",
    "file-loader": "0.11.1",
    "font-awesome": "4.7.0",
    "font-awesome-webpack2": "0.0.6",
    "google-geocoder": "^0.2.1",
    "google-map-react": "^0.24.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "3.0.0",
    "ignore-styles": "5.0.1",
    "ismobilejs": "^0.4.1",
    "jest": "20.0.3",
    "jest-enzyme": "3.0.0",
    "jquery": "3.1.1",
    "jsdom": "11.0.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "4.0.3",
    "lint-staged": "^4.3.0",
    "mocha": "3.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "1.11.0",
    "postcss-loader": "2.0.5",
    "prettier": "^1.7.4",
    "pretty-data": "^0.40.0",
    "rc-checkbox": "2.0.1",
    "rc-collapse": "1.6.12",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-autosuggest": "^9.3.2",
    "react-code-splitting": "^1.2.0",
    "react-cookie": "1.0.5",
    "react-dom": "15.4.1",
    "react-ga": "2.1.2",
    "react-google-maps": "^9.4.5",
    "react-hot-loader": "3.0.0-beta.6",
    "react-modal": "1.6.5",
    "react-number-format": "^3.0.0",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.7",
    "react-select": "1.0.0-rc.3",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "15.6.1",
    "react-waypoint": "7.0.4",
    "recompose": "^0.27.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "sass-loader": "6.0.5",
    "sass-resources-loader": "1.2.1",
    "should": "11.1.2",
    "sinon": "2.3.1",
    "sinon-chai": "2.9.0",
    "style-loader": "0.18.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "2.3.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.1.0-beta.8",
    "webpack-hot-middleware": "2.10.0",
    "webpack-merge": "4.1.0"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", ".c9"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/test/jest/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "modulePaths": ["<rootDir>/src/", "<rootDir>/src/client/components/"],
    "globals": {
      "CONFIG": {
        "isUserLoggedIn": false,
        "googleAnalytics": {
          "enabled": false
        },
        "searchUrl": "/mysearch?q=",
        "katanaRedirectPaths": []
      }
    }
  }
}
