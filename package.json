{
  "name": "sba-gov",
  "version": "0.0.1",
  "description": "The application repository for sba.gov",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node src/app.js --presets es2015",
    "prestart": "npm run clean && npm run lint && npm run format",
    "build": "scripts/build.sh",
    "test": "jest test/",
    "lint": "eslint src --fix",
    "format": "esformatter -i 'src/**/*.js?(x)'",
    "clean": "rm -rf dist && rm -rf public/build",
    "test2": "mocha test2/helpers/browser.js test2/**/*.test.js"
  },
  "repository": {
    "url": "git+https://github.com/sba-ds/sba-gov.git",
    "type": "git"
  },
  "keywords": [
    "sba",
    "ds",
    "node",
    "react"
  ],
  "author": "SBA Digital Services",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sba-ds/sba-gov/issues"
  },
  "homepage": "https://github.com/sba-ds/sba-gov#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
