{
  "name": "sba-gov",
  "version": "0.0.1",
  "description": "The application repository for sba.gov",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development node src/app.js || true",
    "nodemonstart": "nodemon src/app.js --exec babel-node  --presets @babel/preset-env",
    "prestart": "npm run clean",
    "build": "scripts/build.sh all",
    "clean": "rm -rf dist && rm -rf public/build",
    "format": "eslint --fix \"{src,test}/**/*.{js,jsx}\" && prettier --write \"{src,test}/**/*.{json,scss}\"",
    "make-lambda-package": "scripts/make-lambda-package.sh",
    "test": "NODE_ENV=test npm run test-jest && NODE_ENV=test npm run test-mocha",
    "test-jest": "NODE_ENV=test jest -w 1 test/jest",
    "test-mocha": "NODE_ENV=test mocha --compilers js:@babel/register --require @babel/polyfill --require ignore-styles ./test/mocha/helpers/globals.js \"test/mocha/**/*.test.js\"",
    "test-int-mocha": "NODE_ENV=test mocha --compilers js:@babel/register  --require @babel/polyfill --require  ignore-styles ./test/mocha/helpers/globals.js \"test/mocha/**/*.integration-test.js\"",
    "precommit": "npm run test && lint-staged"
  },
  "repository": {
    "url": "git+https://github.com/sba-ds/sba-gov.git",
    "type": "git"
  },
  "keywords": [
    "sba",
    "ds",
    "node",
    "react"
  ],
